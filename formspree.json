{
    "name": "Blaise Mibeck Portfolio Contact Form",
    "email": "blaise.mibeck@example.com",
    "redirectTo": "https://blaise-mibeck.github.io/thank-you.html",
    "accessKey": "",
    "settings": {
        "honeypotEnabled": true,
        "reCaptchaEnabled": false,
        "emailNotifications": true,
        "autoReplyEnabled": true,
        "autoReplyText": "Thank you for your message. I will get back to you as soon as possible.",
        "maxSubmissionsPerMonth": 100
    },
    "fields": {
        "name": {
            "type": "text",
            "required": true,
            "maxLength": 50
        },
        "email": {
            "type": "email",
            "required": true,
            "validationMessage": "Please enter a valid email address"
        },
        "message": {
            "type": "textarea",
            "required": true,
            "maxLength": 1000
        }
    }
}
